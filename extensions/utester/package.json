{
    "package_version": 2,
    "version": "1.0.0",
    "name": "utester",
    "description": "i18n:utester.description",
    "main": "./dist/main.js",
    "dependencies": {
        "fs-extra": "^10.0.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
     
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "panels": {
        "default": {
            "title": "utester - Tester Panel",
            "type": "dockable",
            "main": "dist/panels/main",
            "size": {
                "min-width": 400,
                "min-height": 600,
                "width": 500,
                "height": 700
            }
        }
    },
    "contributions": {
        "scene": {
            "script": "./dist/scene.js"
        },
        "menu": [
            {
                "path": "i18n:menu.panel/utester",
                "label": "i18n:utester.open_panel",
                "message": "open-panel"
            },
            {
                "path": "i18n:menu.develop/utester",
                "label": "i18n:utester.send_to_panel",
                "message": "send-to-panel"
            }
        ],
        "messages": {
            "open-panel": {
                "public": true,
                "methods": [
                    "openPanel"
                ]
            },
            "send-to-panel": {
                "public": true,
                "methods": [
                    "default.hello",
                    "methodOP",
                    "methodSTP"
                ]
            }
        },
        "shortcuts": [
            {
                "message": "open-search-bar",
                "win": "F2",
                "mac": "F2"
            }
        ]
    },
    "author": "Jose Ma Lucero",
    "editor": ">=3.7.3",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    }
}
